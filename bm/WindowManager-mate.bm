FLL_DESCRIPTION="MATE"

FLL_PRE_PROCESSING=(
	"#!/bin/bash"
	"echo \"---- in preprocessing ----\""
        "echo \"Adding MATE repository to /etc/apt/sources.list.d/various.list\""
	"sed -i -r '/mate-/ s/^#+//' /etc/apt/sources.list.d/various.list"
        "apt-get update"
        "apt-get install -y --force-yes mate-archive-keyring"
        "apt-get update"
	"echo \"---- preprocessing done----\""
)

FLL_PACKAGE_DEPMODS=(
)

FLL_PACKAGES=(
      mate-core
      mate-desktop-environment

)


	 